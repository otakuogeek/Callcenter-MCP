{
  "name": "cita-central-ips-backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/server.js",
    "db:init": "ts-node-dev --transpile-only scripts/init-db.ts",
    "db:seed": "ts-node-dev --transpile-only scripts/seed.ts",
    "db:check": "ts-node-dev --transpile-only scripts/check-db.ts",
    "db:migrate": "ts-node-dev --transpile-only scripts/apply-migration.ts",
    "doctors:init-passwords": "ts-node-dev --transpile-only scripts/init-doctor-passwords.ts",
    "elevenlabs:setup": "./scripts/setup-elevenlabs-db.sh",
    "elevenlabs:test": "./scripts/test-elevenlabs.sh",
    "elevenlabs:confirm-calls": "ts-node-dev --transpile-only src/scripts/send-confirmation-calls.ts",
    "sync:initial": "ts-node-dev --transpile-only scripts/syncInitial.ts",
    "sync:calls": "ts-node-dev --transpile-only scripts/syncInitial.ts",
    "test:features": "ts-node-dev --transpile-only scripts/test-new-features.ts",
    "test": "jest --runInBand",
    "archive:calls": "ts-node-dev --transpile-only scripts/archive-calls.ts",
    "recalc:appointments": "ts-node-dev --transpile-only scripts/recalc-appointments.ts",
    "lint": "eslint src/**/*.ts --max-warnings 0 --config .eslintrc.json",
    "lint:fix": "eslint src/**/*.ts --fix --config .eslintrc.json",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "health": "curl -s http://localhost:$PORT/health || true"
  },
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.12.2",
    "@types/bcrypt": "^6.0.0",
    "@types/nodemailer": "^7.0.1",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "labsmobile-sms": "^1.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.0",
    "nodemailer": "^6.10.1",
    "openai": "^6.7.0",
    "pino": "^9.4.0",
    "pino-http": "^10.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.7",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.12",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
